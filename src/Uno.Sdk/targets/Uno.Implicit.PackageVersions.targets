<Project>

	<ItemGroup>
		<_UnoImplicitPackageVersion Include="Uno.WinUI" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.WinUI.Lottie" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.WinUI.Maps" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.WinUI.DevServer" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Resizetizer" Version="$(UnoResizetizerVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Microsoft.Extensions.Logging.Console" Version="$(MicrosoftLoggingVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="SkiaSharp.Skottie" Version="$(SkiaSharpVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="SkiaSharp.Views.Uno.WinUI" Version="$(SkiaSharpVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.WinUI.Lottie" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Logging.OSLog" Version="$(UnoLoggingVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.UniversalImageLoader" Version="$(UnoUniversalImageLoader)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Xamarin.Google.Android.Material" Version="$(AndroidMaterialVersion)" />
		<_UnoImplicitPackageVersion Include="Uno.WinUI.WebAssembly" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Wasm.Bootstrap" Version="$(UnoWasmBootstrapVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Wasm.Bootstrap.DevServer" Version="$(UnoWasmBootstrapVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Logging.WebAssembly.Console" Version="$(UnoLoggingVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Microsoft.Windows.Compatibility" Version="$(WindowsCompatibilityVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.WinUI.Skia.Gtk" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Microsoft.WindowsAppSDK" Version="$(WinAppSdkVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Microsoft.Windows.SDK.BuildTools" Version="$(WinAppSdkBuildToolsVersion)" IsImplicitlyDefined="true" />
	</ItemGroup>

	<ItemGroup Condition="$(UnoCSharpMarkupVersion) != ''">
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Markup.WinUI" Version="$(UnoCSharpMarkupVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Markup.Generators" Version="$(UnoCSharpMarkupVersion)" IsImplicitlyDefined="true" />
	</ItemGroup>

	<ItemGroup Condition="$(UnoExtensionsVersion) != ''">
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Authentication.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Authentication.MSAL.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Authentication.Oidc.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Core.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Hosting.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Http.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Http.Refit" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Localization.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Logging.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Maui.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" Condition="$(UseMaui) == 'true'" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Maui.WinUI.Markup" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" Condition="$(UseMaui) == 'true'" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Navigation.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Navigation.WinUI.Markup" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Reactive.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" Condition="$(Presentation) == 'Mvux'" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Reactive.WinUI.Markup" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" Condition="$(Presentation) == 'Mvux'"  />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Storage.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Serialization.Http" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Serialization.Refit" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Extensions.Toolkit.WinUI" Version="$(UnoExtensionsVersion)" IsImplicitlyDefined="true" />
	</ItemGroup>

	<ItemGroup Condition="$(UnoToolkitVersion) != ''">
		<_UnoImplicitPackageVersion Include="Uno.Toolkit.WinUI" Version="$(UnoToolkitVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Toolkit.WinUI.Cupertino" Version="$(UnoToolkitVersion)" IsImplicitlyDefined="true" Condition="$(UnoTheme) == 'Cupertino'" />
		<_UnoImplicitPackageVersion Include="Uno.Toolkit.WinUI.Material" Version="$(UnoToolkitVersion)" IsImplicitlyDefined="true" Condition="$(UnoTheme) == 'Material'" />
		<_UnoImplicitPackageVersion Include="Uno.Toolkit.WinUI.Material.Markup" Version="$(UnoToolkitVersion)" IsImplicitlyDefined="true" Condition="$(UnoTheme) == 'Material'" />
		<_UnoImplicitPackageVersion Include="Uno.Toolkit.WinUI.Markup" Version="$(UnoToolkitVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Uno.Toolkit.Skia.WinUI" Version="$(UnoToolkitVersion)" IsImplicitlyDefined="true" />
	</ItemGroup>

	<ItemGroup Condition="$(Presentation) == 'Mvvm'">
		<_UnoImplicitPackageVersion Include="CommunityToolkit.Mvvm" Version="$(CommunityToolkitMvvmVersion)" IsImplicitlyDefined="true" />
	</ItemGroup>

	<ItemGroup Condition="$(Presentation) == 'Prism'">
		<_UnoImplicitPackageVersion Include="Prism.Uno.WinUI" Version="$(PrismVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Prism.Uno.WinUI.Markup" Version="$(PrismVersion)" IsImplicitlyDefined="true" />
		<_UnoImplicitPackageVersion Include="Prism.DryIoc.Uno.WinUI" Version="$(PrismVersion)" IsImplicitlyDefined="true" Condition=" $(SingleProject) == 'true' or $(OutputType) == 'WinExe' or $(OutputType) == 'Exe' " />
	</ItemGroup>

	<ItemGroup>
		<PackageVersion Include="@(_UnoImplicitPackageVersion)" Exclude="@(PackageVersion)" />
	</ItemGroup>

</Project>
