<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>$basetargetframework$</TargetFramework>
		<Nullable>enable</Nullable>

		<!-- Xaml fuzzy matching will be removed in Uno 5 to match Windows and also improve performance -->
		<!-- It is recommended to turn it off -->
		<UnoEnableXamlFuzzyMatching>false</UnoEnableXamlFuzzyMatching>
	</PropertyGroup>

	$if$($UseWebAssembly$ == True)<ItemGroup>
		<ProjectReference Include="..\$ext_safeprojectname$.Wasm\$ext_safeprojectname$.Wasm.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Uno.Wasm.Bootstrap.Server" Version="7.0.0-dev.195" />
	</ItemGroup>$endif$

</Project>
